<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias type="data.vo.Student_VO" alias="Student_VO" />
		<typeAlias type="data.vo.Test_VO" alias="Test_VO" />
		<typeAlias type="data.vo.Mypage_VO" alias="Mypage_VO" />
		<typeAlias
			type="data.vo.Professor_education_identity_test_VO"
			alias="Professor_education_identity_test_VO" />
		<typeAlias type="data.vo.Capability_category_VO"
			alias="Capability_category_VO" />
		<typeAlias type="data.vo.Capability_matching_score_VO"
			alias="Capability_matching_score_VO" />
		<typeAlias type="data.vo.Career_test_VO"
			alias="Career_test_VO" />
		<typeAlias type="data.vo.Diagnosis_criteria_VO"
			alias="Diagnosis_criteria_VO" />
		<typeAlias type="data.vo.Education_identity_test_VO"
			alias="Education_identity_test_VO" />
		<typeAlias type="data.vo.Freshman_career_test_VO"
			alias="Freshman_career_test_VO" />
		<typeAlias type="data.vo.Identity_test_category_VO"
			alias="Identity_test_category_VO" />
		<typeAlias type="data.vo.Integration_score_VO"
			alias="Integration_score_VO" />
		<typeAlias type="data.vo.Matching_criteria_VO"
			alias="Matching_criteria_VO" />
		<typeAlias type="data.vo.Preparation_level_VO"
			alias="Preparation_level_VO" />
		<typeAlias type="data.vo.Professor_career_evaluation_VO"
			alias="Professor_career_evaluation_VO" />
		<typeAlias type="data.vo.Professor_VO" alias="Professor_VO" />
		<typeAlias type="data.vo.Professor_weight_VO"
			alias="Professor_weight_VO" />
		<typeAlias type="data.vo.Test_category_VO"
			alias="Test_category_VO" />
		<typeAlias type="data.vo.Mypage_VO" alias="Mypage_VO" />


	</typeAliases>
	<environments default="development">
		<environment id="development">
			<transactionManager type="JDBC" />
			<dataSource type="POOLED">
				<property name="driver" value="com.mysql.cj.jdbc.Driver" />
				<property name="url"
					value="jdbc:mysql://localhost:3306/ncp" />
				<property name="username" value="root" />
				<property name="password" value="1234" />
			</dataSource>
		</environment>
	</environments>
	<mappers>
		<mapper resource="data/mybatis/mappers/test.xml" />
		<mapper resource="data/mybatis/mappers/Student.xml" />
		<mapper resource="data/mybatis/mappers/Admin_Mode.xml" />
		<mapper resource="data/mybatis/mappers/Mypage.xml" />
		<mapper resource="data/mybatis/mappers/Education_identity_test_VO.xml" />
		<mapper resource="data/mybatis/mappers/professor_education_identity_test.xml"/> 
	</mappers>
</configuration>